{
  "name": "losdos",
  "version": "0.0.4",
  "description": "Boilerplate for a simple express web application with views and a REST api.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require blanket",
    "coverage": "./node_modules/.bin/mocha --require blanket -R html-cov > coverage.html",
    "coveralls": "./node_modules/.bin/mocha --require blanket --reporter mocha-lcov-reporter"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coltonw/losdos.git"
  },
  "author": "coltonw@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coltonw/losdos/issues"
  },
  "homepage": "https://github.com/coltonw/losdos",
  "dependencies": {
    "express": "3.x"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "mocha": "~1.18.2",
    "supertest": "~0.12.0",
    "coveralls": "^2.10.0",
    "mocha-lcov-reporter": "0.0.1",
    "blanket": "^1.1.6"
  }
}
